    }],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "illuminate/database": "~5.1",
        "tuupola/slim-basic-auth": "^2.3",
        "firebase/php-jwt": "^5.0",
